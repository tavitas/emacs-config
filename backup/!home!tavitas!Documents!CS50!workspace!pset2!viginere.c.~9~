#include <ctype.h>
#include <string.h>

int main(int argc, string argv[]) {
	if (argc == 2){
		printf("plaintext: ");
		string p = get_string();
		
		int k = atoi(argv[1]);

		for (int i = 0, l = strlen(p);i < l;i++) {
			printf("%c",p[i]);
		}
	} else {
		printf("Usage: ./viginere keyword'\n");
	}
}
