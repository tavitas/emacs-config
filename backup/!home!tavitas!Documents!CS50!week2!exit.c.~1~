#include <cs50.h>
#include <stdio.h>
#include <string.h>
int main(int argc, string argv[]) {
  for (int i = 0; i < argc; i++) {
    //iterate over strings in argv
    for (int j = 0, n = strlen(argv[i]); j < n; j++) {

      //print the j'th characters in current string
      printf("%c\n", argv[i][j]);
    }
    printf("%s\n", argv[i]);
  }
}


