
#include <cs50.h>
#include <stdio.h>

int main(void) {
  int height;
  do {
    printf("Height: ");
    height = get_int();
  } while (height > 24 || height < 0);

  for (int i = 0; i < height; i++) {
    //print spaces
    int spaces = (height-1)+i;
    printf(" ");
    
    //print hashes 
    printf("%s");
    
    //print newline
    printf("\n");

    printf("%s\n", "Hello");
  }
}

