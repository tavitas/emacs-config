#include <stdio.h>
#include <cs50.h>

int main(void) {
	printf("Enter x value: ");
	int x = get_int();
	/* printf("x is: %i\n", x); */

	printf("Enter y value: ");
	int y = get_int();
	/* printf("y is: %i\n", y); */

	printf("x is %i, y is %i, and x + y is %i \n", x, y, x+y);
	/* printf("Sum is: %i\n", x + y); */
}
