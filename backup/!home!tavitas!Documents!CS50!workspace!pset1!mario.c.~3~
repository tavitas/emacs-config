
#include <cs50.h>
#include <stdio.h>

int main(void) {
  printf("Height: ");
  int height = get_int();

  /* int counter; */

  for (int i = 0; i > height; i++) {
    for (int j = 0; j < i; j++) {
      printf("#");
    }
    printf("\n");
  }

  /* while (height > 0) { */
  /*   printf("#"); */
  /*   height--; */
  /* }  */
}

